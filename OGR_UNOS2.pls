create or replace TRIGGER OGR_UNOS2 
BEFORE INSERT ON KURS_U_VALUTI 
FOR EACH ROW
DECLARE 
  EXC1 EXCEPTION;
BEGIN
  
  IF( :NEW.KLS_KUPOVNI > :NEW.KLS_PRODAJNI ) THEN
    RAISE EXC1;
  END IF;
  
EXCEPTION
  WHEN EXC1 THEN
    RAISE_APPLICATION_ERROR(-20000,'KUPOVNI NE SME BITI VECI OD PRODAJNOG');
    
END;